{
  "name": "1j1s-front",
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "version": "3.58.1",
  "private": true,
  "scripts": {
    "prepare": "npm dedupe && ./setup.sh",
    "dev": "next dev",
    "build": "next build",
    "check": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx} --coverageReporters=text --coverageReporters=lcov --coverageReporters=html && open coverage/index.html",
    "e2e:open": "cypress open",
    "e2e": "cypress run --headed",
    "tw": "jest --watch",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "^0.12.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@sentry/nextjs": "~7.50.0",
    "axios": "~1.4.0",
    "axios-cache-interceptor": "~1.0.1",
    "classnames": "~2.3.1",
    "dompurify": "~3.0.1",
    "ioredis": "~5.3.1",
    "joi": "^17.8.4",
    "lodash.debounce": "~4.0.8",
    "marked": "~5.0.0",
    "next": "^13.2.4",
    "next-joi": "~2.2.1",
    "phone": "~3.1.35",
    "pino": "~8.11.0",
    "pino-sentry": "~0.14.0",
    "react": "~18.2.0",
    "react-autosuggest": "~10.1.0",
    "react-dom": "~18.2.0",
    "react-instantsearch-hooks-web": "~6.43.0",
    "redis": "~4.6.5",
    "reflect-metadata": "~0.1.13",
    "sharp": "~0.32.0",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "~1.131.0",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~14.0.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/dompurify": "~3.0.0",
    "@types/jest": "^29.4.1",
    "@types/lodash.debounce": "~4.0.7",
    "@types/marked": "~4.3.0",
    "@types/node": "~18.16.0",
    "@types/react": "~18.2.0",
    "@types/react-autosuggest": "~10.1.6",
    "@types/react-dom": "~18.2.0",
    "@types/uuid": "~9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.59.0",
    "cypress": "~12.11.0",
    "eslint": "~8.39.0",
    "eslint-config-next": "^13.2.4",
    "eslint-plugin-cypress": "~2.13.1",
    "eslint-plugin-testing-library": "~5.10.2",
    "jest": "~29.5.0",
    "jest-environment-jsdom": "~29.5.0",
    "next-test-api-route-handler": "~3.1.6",
    "nock": "~13.3.0",
    "sass": "~1.62.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.2"
  },
  "overrides": {
    "@socialgouv/eslint-config-recommended": {
      "eslint": "^8.28.0"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.js": "eslint --fix"
  }
}
