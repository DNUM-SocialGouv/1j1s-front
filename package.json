    "lodash.debounce": "4.0.8",
    "markdown-it": "14.1.0",
    "markdown-it-anchor": "9.2.0",
    "next": "14.2.9",
    "next-joi": "2.2.1",
    "phone": "3.1.50",
    "pino": "9.4.0",
    "pino-sentry": "0.14.1",
    "react": "18.3.1",
    "react-autosuggest": "10.1.0",
    "react-dom": "18.3.1",
    "react-instantsearch": "7.13.1",
    "react-instantsearch-router-nextjs": "7.13.1",
    "redis": "4.7.0",
    "reflect-metadata": "0.2.2",
    "sharp": "0.33.5",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "1.131.0",
