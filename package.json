    "@socialgouv/eslint-config-recommended": {
      "eslint": "^8.28.0"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.js": "eslint --fix"
  }
