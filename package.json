{
  "name": "1j1s-front",
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "prepare": "./setup.sh",
    "dev": "next dev",
    "build": "next build",
    "check": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx} --coverageReporters=text --coverageReporters=lcov --coverageReporters=html && open coverage/index.html",
    "e2e:open": "cypress open",
    "e2e": "cypress run --headed",
    "tw": "jest --watch",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build"
  },
  "dependencies": {
    "@meilisearch/instant-meilisearch": "~0.11.0",
    "@next/bundle-analyzer": "~13.1.6",
    "@sentry/nextjs": "~7.38.0",
    "axios": "~1.3.3",
    "classnames": "~2.3.1",
    "dompurify": "~3.0.0",
    "ioredis": "~5.3.0",
    "joi": "~17.7.0",
    "lodash.debounce": "~4.0.8",
    "marked": "~4.2.12",
    "next": "~13.1.6",
    "next-joi": "~2.2.1",
    "phone": "~3.1.33",
    "react": "~18.2.0",
    "react-autosuggest": "~10.1.0",
    "react-dom": "~18.2.0",
    "react-instantsearch-hooks-web": "~6.40.0",
    "redis": "~4.6.4",
    "reflect-metadata": "~0.1.13",
    "sharp": "~0.31.0",
    "uuid": "~9.0.0"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "~1.131.0",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~13.4.0",
    "@testing-library/user-event": "~14.4.3",
    "@types/dompurify": "~2.4.0",
    "@types/jest": "~29.4.0",
    "@types/lodash.debounce": "~4.0.7",
    "@types/marked": "~4.0.3",
    "@types/node": "~18.14.0",
    "@types/react": "~18.0.27",
    "@types/react-autosuggest": "~10.1.6",
    "@types/react-dom": "~18.0.5",
    "@types/uuid": "~9.0.0",
    "@typescript-eslint/eslint-plugin": "~5.52.0",
    "cypress": "~12.6.0",
    "eslint": "~8.34.0",
    "eslint-config-next": "~13.1.6",
    "eslint-plugin-cypress": "~2.12.1",
    "eslint-plugin-testing-library": "~5.10.0",
    "jest": "~29.4.1",
    "jest-environment-jsdom": "~29.4.1",
    "next-test-api-route-handler": "~3.1.6",
    "nock": "~13.3.0",
    "sass": "~1.58.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5"
  },
  "overrides": {
    "@socialgouv/eslint-config-recommended": {
      "eslint": "^8.28.0"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix",
    "*.js": "eslint --fix"
  }
}
